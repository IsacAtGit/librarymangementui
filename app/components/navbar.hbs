<ol class="breadcrumb">
    
        
    
    <li class="breadcrumb-item"><LinkTo @route="index">Home</LinkTo></li>
    <li class="breadcrumb-item"><LinkTo @route="cart">cart</LinkTo></li>
   {{log this.userid}}
   {{log "nav"}}
  {{#if this.userid}}
    <li class="breadcrumb-item"><LinkTo @route="profile">profile</LinkTo></li>
    <li class="breadcrumb-item"><LinkTo @route="login" {{on "click" this.logout}}>logout</LinkTo></li>
  {{else}}
    <li class="breadcrumb-item"><LinkTo @route="login" >login</LinkTo></li>
    
  {{/if}}
    {{yield}}
</ol>